{
  "name": "ans-sdk",
  "version": "0.1.0",
  "description": "SDK for the Agent Name Service (ANS) - The DNS for AI Agents",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src/"
  },
  "dependencies": {
    "ans-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "keywords": ["ai", "agents", "registry", "sdk"],
  "license": "MIT"
}
